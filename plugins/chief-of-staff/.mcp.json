{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp", "--headless", "--browser", "chrome"]
    },
    "parcel": {
      "command": "node",
      "args": ["${CLAUDE_PLUGIN_ROOT}/mcp-server/parcel/dist/server.js"],
      "env": {
        "PARCEL_API_KEY": "${PARCEL_API_KEY}"
      }
    }
  }
}
