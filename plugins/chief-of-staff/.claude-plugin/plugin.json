{
  "name": "chief-of-staff",
  "version": "1.5.0",
  "description": "The orchestrator plugin for personal productivity - inbox triage, package tracking, reminder creation, newsletter management, and more",
  "author": {
    "name": "Omar Shahine"
  },
  "license": "MIT",
  "keywords": [
    "email",
    "inbox",
    "triage",
    "packages",
    "parcel",
    "reminders",
    "newsletters",
    "unsubscribe",
    "productivity",
    "orchestrator"
  ],
  "mcpServers": {
    "fastmail": {
      "type": "http",
      "url": "${FASTMAIL_MCP_URL}"
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp", "--headless", "--browser", "chrome"]
    },
    "parcel": {
      "command": "${CLAUDE_PLUGIN_ROOT}/scripts/parcel-server.sh"
    }
  }
}
