{
  "name": "parcel-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Parcel delivery tracking",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild src/server.ts --bundle --platform=node --format=esm --outfile=bundle/server.mjs",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "esbuild": "^0.27.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
